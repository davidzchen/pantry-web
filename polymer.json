{
  "entrypoint": "index.html",
  "shell": "src/pantry-app.html",
  "fragments": [
    "src/pantry-home.html",
    "src/pantry-package.html",
    "src/pantry-results.html",
    "src/pantry-docs.html",
    "src/pantry-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*"
  ],
  "lint": {
    "rules": ["polymer-2-hybrid"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
